curl -XPUT 'elasticsearch:9200/_ingest/pipeline/docker_logs_pipeline?pretty' -H 'Content-Type: application/json' -d'
{
  "pipeline": {
    "description" : "parse multiple patterns",
    "processors": [
      {
        "grok": {
          "field": "message",
